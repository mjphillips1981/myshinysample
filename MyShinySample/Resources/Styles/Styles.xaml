	 <Style
		  TargetType="Button">
		  <Setter
				Property="FontFamily"
				Value="{OnPlatform iOS='', Default='OpenSansRegular'}" />
		  <Setter
				Property="FontSize"
				Value="Small" />
		  <Setter
				Property="CornerRadius"
				Value="0" />
		  <Setter
